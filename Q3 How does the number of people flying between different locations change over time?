'''
Q3. How does the number of people flying between different locations change over time? 
Years used: 1988, 1989, 1990, 1991 and 1992
Airports used: LAX(West), JFK(East), DEN(South) and DWF(Central)
Data was split into seasons to make processing faster and easier
'''

import pandas as pd
import matplotlib.pyplot as plt

data_1988 = pd.read_csv(r'C:\Users\HOME\Desktop\2195 Programming Coursework\1988.csv.bz2')

#Filter for 1988 Spring and filter for flights to/from LAX <-> JFK airport 
df_1988_Spring_WE = pd.DataFrame(data_1988, columns=['Month', 'Origin', 'Dest'])
df_1988_Spring_WE = df_1988_Spring_WE[(df_1988_Spring_WE['Month'] >= 3) & (df_1988_Spring_WE['Month'] <= 5)]
df_1988_Spring_WE = df_1988_Spring_WE[df_1988_Spring_WE['Origin'] == 'LAX']
df_1988_Spring_WE = df_1988_Spring_WE[df_1988_Spring_WE['Dest'] == 'JFK']
print(df_1988_Spring_WE)

df_1988_Spring_EW = pd.DataFrame(data_1988, columns=['Month', 'Origin', 'Dest'])
df_1988_Spring_EW = df_1988_Spring_EW[(df_1988_Spring_EW['Month'] >= 3) & (df_1988_Spring_EW['Month'] <= 5)]
df_1988_Spring_EW = df_1988_Spring_EW[df_1988_Spring_EW['Origin'] == 'JFK']
df_1988_Spring_EW = df_1988_Spring_EW[df_1988_Spring_EW['Dest'] == 'LAX']
print(df_1988_Spring_EW)

#Filter for 1988 Spring and filter for flights to/from DFW <-> DEN airport 
df_1988_Spring_SC = pd.DataFrame(data_1988, columns=['Month', 'Origin', 'Dest'])
df_1988_Spring_SC = df_1988_Spring_SC[(df_1988_Spring_SC['Month'] >= 3) & (df_1988_Spring_SC['Month'] <= 5)]
df_1988_Spring_SC = df_1988_Spring_SC[df_1988_Spring_SC['Origin'] == 'DFW']
df_1988_Spring_SC = df_1988_Spring_SC[df_1988_Spring_SC['Dest'] == 'DEN']
print(df_1988_Spring_SC)

df_1988_Spring_CS = pd.DataFrame(data_1988, columns=['Month', 'Origin', 'Dest'])
df_1988_Spring_CS = df_1988_Spring_CS[(df_1988_Spring_CS['Month'] >= 3) & (df_1988_Spring_CS['Month'] <= 5)]
df_1988_Spring_CS = df_1988_Spring_CS[df_1988_Spring_CS['Origin'] == 'DEN']
df_1988_Spring_CS = df_1988_Spring_CS[df_1988_Spring_CS['Dest'] == 'DFW']
print(df_1988_Spring_CS)

##Filter for 1988 Summer and filter for flights to/from LAX <-> JFK airport 
df_1988_Summer_WE = pd.DataFrame(data_1988, columns=['Month', 'Origin', 'Dest'])
df_1988_Summer_WE = df_1988_Summer_WE[(df_1988_Summer_WE['Month'] >= 6) & (df_1988_Summer_WE['Month'] <= 8)]
df_1988_Summer_WE = df_1988_Summer_WE[df_1988_Summer_WE['Origin'] == 'LAX']
df_1988_Summer_WE = df_1988_Summer_WE[df_1988_Summer_WE['Dest'] == 'JFK']
print(df_1988_Summer_WE)

df_1988_Summer_EW = pd.DataFrame(data_1988, columns=['Month', 'Origin', 'Dest'])
df_1988_Summer_EW = df_1988_Summer_EW[(df_1988_Summer_EW['Month'] >= 6) & (df_1988_Summer_EW['Month'] <= 8)]
df_1988_Summer_EW = df_1988_Summer_EW[df_1988_Summer_EW['Origin'] == 'JFK']
df_1988_Summer_EW = df_1988_Summer_EW[df_1988_Summer_EW['Dest'] == 'LAX']
print(df_1988_Summer_EW)

##Filter for 1988 Summer and filter for flights to/from DFW <-> DEN airport 
df_1988_Summer_SC = pd.DataFrame(data_1988, columns=['Month', 'Origin', 'Dest'])
df_1988_Summer_SC = df_1988_Summer_SC[(df_1988_Summer_SC['Month'] >= 6) & (df_1988_Summer_SC['Month'] <= 8)]
df_1988_Summer_SC = df_1988_Summer_SC[df_1988_Summer_SC['Origin'] == 'DFW']
df_1988_Summer_SC = df_1988_Summer_SC[df_1988_Summer_SC['Dest'] == 'DEN']
print(df_1988_Summer_SC)

