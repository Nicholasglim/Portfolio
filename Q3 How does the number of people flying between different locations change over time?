###Filter for 1988 Autumn and filter for flights to/from DFW <-> DEN airport 
df_1988_Autumn_SC = pd.DataFrame(data_1988, columns=['Month', 'Origin', 'Dest'])
df_1988_Autumn_SC = df_1988_Autumn_SC[(df_1988_Autumn_SC['Month'] >= 9) & (df_1988_Autumn_SC['Month'] <= 11)]
df_1988_Autumn_SC = df_1988_Autumn_SC[df_1988_Autumn_SC['Origin'] == 'DFW']
df_1988_Autumn_SC = df_1988_Autumn_SC[df_1988_Autumn_SC['Dest'] == 'DEN']
print(df_1988_Autumn_SC)

df_1988_Autumn_CS = pd.DataFrame(data_1988, columns=['Month', 'Origin', 'Dest'])
df_1988_Autumn_CS = df_1988_Autumn_CS[(df_1988_Autumn_CS['Month'] >= 9) & (df_1988_Autumn_CS['Month'] <= 11)]
df_1988_Autumn_CS = df_1988_Autumn_CS[df_1988_Autumn_CS['Origin'] == 'DEN']
df_1988_Autumn_CS = df_1988_Autumn_CS[df_1988_Autumn_CS['Dest'] == 'DFW']
print(df_1988_Autumn_CS)

####Filter for 1988 Winter and filter for flights to/from LAX <-> JFK airport 
df_1988_Winter_WE = pd.DataFrame(data_1988, columns=['Month', 'Origin', 'Dest'])
df_1988_Winter_WE = df_1988_Winter_WE[(df_1988_Winter_WE['Month'] >= 12) | (df_1988_Winter_WE['Month'] <= 2)]
df_1988_Winter_WE = df_1988_Winter_WE[df_1988_Winter_WE['Origin'] == 'LAX']
df_1988_Winter_WE = df_1988_Winter_WE[df_1988_Winter_WE['Dest'] == 'JFK']
