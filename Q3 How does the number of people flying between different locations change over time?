df_1988_Summer_CS = df_1988_Summer_CS[(df_1988_Summer_CS['Month'] >= 6) & (df_1988_Summer_CS['Month'] <= 8)]
df_1988_Summer_CS = df_1988_Summer_CS[df_1988_Summer_CS['Origin'] == 'DEN']
df_1988_Summer_CS = df_1988_Summer_CS[df_1988_Summer_CS['Dest'] == 'DFW']
print(df_1988_Summer_CS)

###Filter for 1988 Autumn and filter for flights to/from LAX <-> JFK airport 
df_1988_Autumn_WE = pd.DataFrame(data_1988, columns=['Month', 'Origin', 'Dest'])
df_1988_Autumn_WE = df_1988_Autumn_WE[(df_1988_Autumn_WE['Month'] >= 9) & (df_1988_Autumn_WE['Month'] <= 11)]
df_1988_Autumn_WE = df_1988_Autumn_WE[df_1988_Autumn_WE['Origin'] == 'LAX']
df_1988_Autumn_WE = df_1988_Autumn_WE[df_1988_Autumn_WE['Dest'] == 'JFK']
print(df_1988_Autumn_WE)

df_1988_Autumn_EW = pd.DataFrame(data_1988, columns=['Month', 'Origin', 'Dest'])
df_1988_Autumn_EW = df_1988_Autumn_EW[(df_1988_Autumn_EW['Month'] >= 9) & (df_1988_Autumn_EW['Month'] <= 11)]
df_1988_Autumn_EW = df_1988_Autumn_EW[df_1988_Autumn_EW['Origin'] == 'JFK']
df_1988_Autumn_EW = df_1988_Autumn_EW[df_1988_Autumn_EW['Dest'] == 'LAX']
